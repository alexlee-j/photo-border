{
  "$schema": "https://tauri.app/v2/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "fs:default",
    {
      "identifier": "fs:allow-read-file",
      "allow": [
        {
          "path": "/**/*"
        }
      ]
    }
  ]
}
